<log4net>
  <!-- Define some output appenders -->

  <appender name="RedisLogAppender" type="Log4Redis.RedisPubAppender,Log4Redis">
    <param name="Topic" value="Test"/>
    <param name="Host" value="csj-oq-kgd01.sdcorp.global.sandisk.com"/>
    <param name="Port" value="6379"/>

    <layout type="log4net.Layout.PatternLayout">
      <conversionPattern value="%date %-5level %logger [%thread] - %message" />
    </layout>
    <filter type="log4net.Filter.LevelRangeFilter">
      <levelMin value="DEBUG"/>
    </filter>
  </appender>

  <!-- Setup the root category, add the appenders and set the default level -->
  <root>
    <level value="ALL"/>
    <!--<appender-ref ref="RollingFileLogAppender"/>-->
    <appender-ref ref="RedisLogAppender" />
  </root>
  <logger name="ConsoleApp1">
    <!--<appender-ref ref="EventLogAppender" />-->
  </logger>
</log4net>